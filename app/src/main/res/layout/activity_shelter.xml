<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ShelterActivity">

    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="44dp"
        android:layout_height="33dp"
        android:layout_alignParentEnd="true"
        android:layout_alignTop="@+id/textView8"
        android:layout_marginTop="6dp"
        app:srcCompat="@drawable/direction" />

    <ImageView
        android:id="@+id/imageView5"
        android:layout_width="44dp"
        android:layout_height="34dp"
        android:layout_alignBottom="@+id/textView7"
        android:layout_alignParentEnd="true"
        app:srcCompat="@drawable/direction" />

    <ImageView
        android:id="@+id/imageView4"
        android:layout_width="44dp"
        android:layout_height="33dp"
        android:layout_alignParentEnd="true"
        android:layout_alignTop="@+id/textView5"
        app:srcCompat="@drawable/direction" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/imageView"
        android:layout_centerHorizontal="true"
        android:layout_marginBottom="-41dp"
        android:text="Nearest Evacuation Shelters"
        android:textAlignment="center"
        android:textColor="@color/colorPrimaryDark"
        android:textSize="26sp"
        android:textStyle="bold" />

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="wrap_content"
        android:layout_height="232dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginTop="41dp"
        app:srcCompat="@drawable/map_image" />

    <EditText
        android:id="@+id/editText7"
        style="@style/Widget.AppCompat.AutoCompleteTextView"
        android:layout_width="61dp"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_alignStart="@+id/editText8"
        android:layout_marginBottom="27dp"
        android:ems="10"
        android:inputType="none"
        android:text="ES3"
        android:textSize="26sp"
        android:textStyle="bold" />

    <EditText
        android:id="@+id/editText8"
        style="@style/Widget.AppCompat.AutoCompleteTextView"
        android:layout_width="61dp"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_alignStart="@+id/editText9"
        android:layout_marginBottom="95dp"
        android:ems="10"
        android:inputType="none"
        android:text="ES2"
        android:textSize="26sp"
        android:textStyle="bold" />

    <EditText
        android:id="@+id/editText9"
        style="@android:style/Widget.AutoCompleteTextView"
        android:layout_width="61dp"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_alignParentStart="true"
        android:layout_marginBottom="165dp"
        android:layout_marginStart="13dp"
        android:ems="10"
        android:inputType="none"
        android:text="ES1"
        android:textSize="26sp"
        android:textStyle="bold" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@+id/editText7"
        android:layout_alignStart="@+id/textView7"
        android:text="600m away, 9-10min"
        android:textColor="@color/common_google_signin_btn_text_dark_focused"
        android:textSize="18sp" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@+id/editText8"
        android:layout_alignStart="@+id/textView5"
        android:text="500m away, 6-7 min"
        android:textColor="@color/common_google_signin_btn_text_dark_focused"
        android:textSize="18sp" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="165dp"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@+id/editText9"
        android:layout_alignParentStart="true"
        android:layout_marginStart="83dp"
        android:text="400m away, 5-6 min"
        android:textColor="@color/common_google_signin_btn_text_dark_focused"
        android:textSize="18sp" />

    <TextView
        android:id="@+id/textView8"
        android:layout_width="88dp"
        android:layout_height="49dp"
        android:layout_alignBottom="@+id/editText7"
        android:layout_alignStart="@+id/textView9"
        android:text="Occupancy 3/70"
        android:textAlignment="center"
        android:textColor="@color/common_google_signin_btn_text_dark_focused"
        android:textSize="17sp" />

    <TextView
        android:id="@+id/textView9"
        android:layout_width="88dp"
        android:layout_height="49dp"
        android:layout_alignBottom="@+id/editText8"
        android:layout_alignStart="@+id/textView10"
        android:text="Occupancy 70/80"
        android:textAlignment="center"
        android:textColor="@color/common_google_signin_btn_text_dark_focused"
        android:textSize="17sp" />

    <TextView
        android:id="@+id/textView10"
        android:layout_width="88dp"
        android:layout_height="49dp"
        android:layout_alignBottom="@+id/editText9"
        android:layout_toEndOf="@+id/textView4"
        android:text="Occupancy 60/120"
        android:textAlignment="center"
        android:textColor="@color/common_google_signin_btn_text_dark_focused"
        android:textSize="17sp" />

    <Button
        android:id="@+id/button_profile"
        android:layout_width="99dp"
        android:layout_height="33dp"
        android:layout_alignParentBottom="true"
        android:layout_alignParentEnd="true"
        android:background="#e3e2ea"
        android:padding="3dp"
        android:text="PROFILE" />

    <Button
        android:id="@+id/button_friends"
        android:layout_width="99dp"
        android:layout_height="33dp"
        android:layout_alignParentBottom="true"
        android:layout_alignParentEnd="true"
        android:layout_marginEnd="95dp"
        android:background="#e3e2ea"
        android:padding="3dp"
        android:text="@string/nav_friends" />

    <Button
        android:id="@+id/button_shelter"
        android:layout_width="99dp"
        android:layout_height="33dp"
        android:layout_alignParentBottom="true"
        android:layout_toEndOf="@+id/button_alert"
        android:background="#e3e2ea"
        android:padding="3dp"
        android:text="SHELTER" />

    <Button
        android:id="@+id/button_alert"
        android:layout_width="99dp"
        android:layout_height="33dp"
        android:layout_alignParentBottom="true"
        android:layout_alignParentStart="true"
        android:background="#e3e2ea"
        android:padding="3dp"
        android:text="ALERT" />

</RelativeLayout>